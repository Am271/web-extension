{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/Logo16.png",
      "32": "/images/Logo32.png",
      "48": "/images/Logo48.png",
      "128": "/images/Logo128.png"
    }
  }
}